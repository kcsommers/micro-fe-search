    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.0",
    "typescript": "^4.5.2",
